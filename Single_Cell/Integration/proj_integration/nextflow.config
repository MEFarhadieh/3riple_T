profiles {
  standard { process.executor = 'local' }
  slurm {
    process.executor = 'slurm'
    process.queue    = 'short'    
    process.memory   = '48 GB'
    process.cpus     = 4
    process.time     = '12h'
  }
}
conda {
  enabled = true
}
workDir = "$LAB_SCRATCH/nf-work"
timeline.enabled = true
trace.enabled    = true
report.enabled   = true
